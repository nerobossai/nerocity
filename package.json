{
  "name": "martian.fun",
  "version": "0.0.1-alpha",
  "scripts": {
    "dev": "env-cmd -f .env.development next dev --port 4536",
    "build": "env-cmd -f .env.production next build",
    "build:dev": "env-cmd -f .env.development next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build:dev",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next .swc out coverage",
    "lint": "next lint",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty",
    "commit": "cz",
    "postbuild": "next-sitemap"
  },
  "pre-commit": [
    "lint"
  ],
